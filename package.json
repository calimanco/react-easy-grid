{
  "name": "@calimanco/react-easy-grid",
  "version": "0.0.0",
  "description": "A react component easy for grid layout.",
  "keywords": [
    "react",
    "grid",
    "layout",
    "typescript"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-easy-grid.umd.cjs",
  "module": "./dist/react-easy-grid.js",
  "exports": {
    ".": {
      "import": "./dist/react-easy-grid.js",
      "require": "./dist/react-easy-grid.umd.cjs"
    }
  },
  "author": "calimanco <calimanco@qq.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calimanco/react-easy-grid.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:demo": "vite build -c vite.demo.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "commit": "commit",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "coverage": "vitest run --coverage",
    "report-coverage": "./coverage/lcov.info | coveralls",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "antd": "^4.24.16",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/prompt-cli": "^19.3.1",
    "@eslint/js": "^9.3.0",
    "@stylistic/eslint-plugin": "^1.8.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.7",
    "@types/react": "^17.0.80",
    "@types/react-color": "^3.0.12",
    "@types/react-dom": "^17.0.25",
    "@vitejs/plugin-legacy": "^5.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "sass": "^1.77.0",
    "semantic-release": "^23.1.1",
    "stylelint": "^16.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "terser": "^5.31.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.10.0",
    "vite": "^5.2.0",
    "vitest": "^1.6.0"
  }
}
